<Page x:Class="SistemaDeArbitraje.Paginas.ArticulosRechazados"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SistemaDeArbitraje.Paginas"
      xmlns:uc="clr-namespace:SistemaDeArbitraje.UserControls"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ArticulosRechazados">

    <ScrollViewer HorizontalScrollBarVisibility="Auto" Background="AliceBlue">
        <ItemsControl Name="tablaArticulos">
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <uc:ArticuloDataControl/>
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </ScrollViewer>
</Page>
